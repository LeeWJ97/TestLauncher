# -*- coding:utf-8 -*-
import json, threading, base64
import time
from datetime import datetime
import urllib.parse
import requests
import tkinter.messagebox
from tkinter import *
from tkinter import scrolledtext

import os
from UIcase import Run


class UI:
    def __init__(self):
        self.rootpath = os.getcwd()
        root = Tk()
        root.title('CSSTestLauncher 2.1')
        root.resizable(0, 0)  # 禁止缩放
        root.wm_attributes('-topmost', 1)  # 总在最前
        self.root = root
        self.initfile()

        Label(root,text='LaunchRow').grid(row=0,column=0,sticky=N)

        #打开的行数
        v1=StringVar()
        v1.set('2')
        self.e1 = Entry(root,width=20,textvariable=v1)
        self.e1.grid(row=0,column=1,columnspan=4,sticky=W)  #跨越4列

        #浏览器类型
        browsertype = [('Chrome', 1), ('Firefox', 2), ('Edge', 3), ('IE', 4)]
        self.browserv = IntVar()
        self.browserv.set(1)  #默认Chrome
        for browser, num in browsertype:
            Radiobutton(root, text=browser, variable=self.browserv, value=num,command=self.ui_setproxycheck_frombrowsertype).grid(row=3, column=num-1,sticky=W)

        #代理IP:端口
        v2=StringVar()
        v2.set('127.0.0.1:8080')
        self.e2 = Entry(root,width=20,textvariable=v2)
        self.e2.grid(row=4,column=1,columnspan=4,sticky=W)  #跨越4列

        #是否代理勾选框
        self.ifproxyv = IntVar()
        self.ifproxycheck = Checkbutton(root, text="Proxy", variable=self.ifproxyv,onvalue=1, offvalue=0,command=self.ui_setproxytext_fromproxycheck)
        self.ifproxycheck.bind(self.ui_setproxytext_fromproxycheck())
        self.ifproxycheck.grid(row=4, column=0, sticky=W)


        #开始按钮
        Button(root, text='Launch', width=8, command=lambda: self.ui_launch(self.e1.get(),self.browserv.get(),self.ifproxyv.get(),self.e2.get())).grid(row=5, column=1, sticky=W)

        root.mainloop()

    def ui_launch(self,rownumstr,type,proxy,proxyipport):
        #print(rownumstr,type,proxy,proxyipport)
        try:
            rownumlist = rownumstr.split(',')
            for i in rownumlist:
                if not isinstance(int(i),int):
                    raise Exception
        except Exception as e:
            print(e)
            tkinter.messagebox.showerror('Error', f"Wrong LaunchRow format! should be like that: 2 or 12,13,17")
            return

        proxy = True if proxy == 1 else False
        print((rownumlist,type,proxy,proxyipport,self.rootpath))
        threading.Thread(target=Run.run,args=(rownumlist,type,proxy,proxyipport,self.rootpath)).start()

    def ui_setproxycheck_frombrowsertype(self):
        #print(self.browserv.get())
        ifcheckproxydisabled =  'normal' if self.browserv.get()==1 else 'disabled'  #除了chrome外，把proxy disable掉
        self.ifproxycheck['state'] = ifcheckproxydisabled  #改变ifproxycheck状态
        if ifcheckproxydisabled == 'disabled':
            self.ifproxyv.set(0)   #取消勾选
            self.e2['state'] = 'disabled'
        else:
            self.e2['state'] = 'normal'
        if self.browserv.get()==1:
            self.e2['state'] = 'disabled'

    def ui_setproxytext_fromproxycheck(self):
        try:
            #print(self.ifproxyv.get())
            if self.ifproxyv.get() == 0:
                self.e2['state'] = 'disabled'
            else:
                self.e2['state'] = 'normal'
        except:
            pass

    def initfile(self):
        initflag = 0
        # 图标
        try:
            icondata = b''
            icondata = base64.b64decode(icondata)
            with open(r'CTL.ico', 'wb') as f:
                f.write(icondata)
            self.root.iconbitmap(r'CTL.ico')
        except:
            try:
                self.root.iconbitmap(icondata)
            except:
                pass

        #文件夹
        needfolder = [rf'{self.rootpath}/data',rf'{self.rootpath}/webdriver']
        for n in needfolder:
            if not os.path.exists(n):
                initflag += 1
                os.mkdir(n)

        #excel文件
        excelfile = b''
        excelfilelocation = rf'{self.rootpath}/data/data.xlsx'
        if not os.path.exists(excelfilelocation):
            initflag += 1
            with open(excelfilelocation,'wb') as f:
                f.write(base64.b64decode(excelfile))

        #webdriver readme文件
        readmetext = '''chromedriver.exe: https://npm.taobao.org/mirrors/chromedriver/
msedgedriver.exe (edge): https://developer.microsoft.com/en-us/microsoft-edge/tools/webdriver/
geckodriver.exe (firefox): http://ftp.mozilla.org/pub/firefox/releases/
IEDriverServer.exe: https://selenium-release.storage.googleapis.com/index.html

Check browser version: open cmd, input appwiz.cpl, and enter.
Replace a correct version into this folder.        
        '''
        readmelocation = rf'{self.rootpath}/webdriver/readme.txt'
        if not os.path.exists(readmelocation):
            initflag += 1
            with open(readmelocation,'w',encoding='utf8') as f:
                f.write(readmetext)

        if initflag > 0:
            tkinter.messagebox.showinfo('init', f"init successfully, please check webdriver in webdriver folder and check login info in data folder")

if __name__ == '__main__':
    UI()